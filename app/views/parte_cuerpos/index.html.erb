<h1>Listing parte_cuerpos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parte_cuerpos.each do |parte_cuerpo| %>
      <tr>
        <td><%= parte_cuerpo.nombre %></td>
        <td><%= link_to 'Show', parte_cuerpo %></td>
        <td><%= link_to 'Edit', edit_parte_cuerpo_path(parte_cuerpo) %></td>
        <td><%= link_to 'Destroy', parte_cuerpo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Parte cuerpo', new_parte_cuerpo_path %>
